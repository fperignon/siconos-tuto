 less /tmp/requirements.frozen.txt
 #!/bin/bash
 git clone https://github.com/siconos/siconos.git
 mkdir build
 cd build
 ls /srv/venv/*/*
 ls /srv/venv/share/python-wheels/*.so
ls /usr/lib/python3.6/config-3.6m-x86_64-linux-gnu/*.so
python3 ../siconos/cmake/explore_python_config.py
 cmake ../siconos -DUSER_OPTIONS_FILE=/home/jovyan/siconos_default.cmake -DPYTHON_LIBRARY=/usr/lib/x86_64-linux-gnu/libpython3.6m.so
 make -j 4
